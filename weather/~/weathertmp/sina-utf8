		<script type="text/javascript">
		function chgCityMenu(Id){
			for(var i=1;i<=2;i++){
				document.getElementById("chgCityMenu_t"+i).className = "caplink";
				document.getElementById("chgCityMenu_c"+i).style.display = "none";
			}
			if(Id==1){
				document.getElementById("chgCityMenu_t1").className = "caplinkOn";
				document.getElementById("chgCityMenu_c1").style.display = "block";
				document.getElementById("chgCityMenu_t2").className = "caplink";
				document.getElementById("chgCityMenu_c2").style.display = "none";
			}else if(Id==2){
				document.getElementById("chgCityMenu_t2").className = "caplinkOn";
				document.getElementById("chgCityMenu_c2").style.display = "block";
				document.getElementById("chgCityMenu_t1").className = "caplink";
				document.getElementById("chgCityMenu_c1").style.display = "none";
			}
		}
		//chgCityMenu(1);//设置默认开启
		</script>
		
